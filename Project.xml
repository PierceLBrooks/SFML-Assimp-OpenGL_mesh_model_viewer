<?xml version="1.0" encoding="UTF-8"?>
<buildster directory="build" distribution="../Distribution" cpp="17">
  <project directory="../Project">
    <dependencies>
      <dependency>
        <label>glew</label>
        <subpath>build</subpath>
        <remote>
          <url>https://github.com/Perlmint/glew-cmake.git</url>
          <git_repo>
            <branch>glew-cmake-release</branch>
            <credentials>
              <username>username</username>
              <password>password</password>
            </credentials>
          </git_repo>
        </remote>
        <build>
          <arguments>
            <argument>-DCMAKE_POLICY_DEFAULT_CMP0074=NEW</argument>
          </arguments>
          <cmake>
            <generator>
              <switch id="BUILDSTER_OS">
                <case check="Windows">MinGW Makefiles</case>
                <case check="Linux">Unix Makefiles</case>
                <case check="Darwin">Xcode</case>
                <default><quit></quit></default>
              </switch>
            </generator>
            <source>..</source>
          </cmake>
        </build>
        <exports>
          <export type="all">
            <key>GLEW_ROOT</key>
            <value><install></install></value>
          </export>
          <export type="headers">
            <key>GLEW_INCLUDE_DIR</key>
            <value><install></install>/include</value>
          </export>
          <export type="libraries">
            <key>GLEW_LIBRARIES</key>
            <value><install></install>/lib</value>
          </export>
        </exports>
        <imports>
          <import>glew</import>
        </imports>
      </dependency>
      <dependency>
        <label>sfml</label>
        <subpath>build</subpath>
        <remote>
          <url>https://github.com/SFML/SFML.git</url>
          <git_repo>
            <branch>2.1</branch>
            <credentials>
              <username>username</username>
              <password>password</password>
            </credentials>
          </git_repo>
        </remote>
        <build>
          <arguments>
            <argument>-DCMAKE_POLICY_DEFAULT_CMP0074=NEW</argument>
          </arguments>
          <cmake>
            <generator>
              <switch id="BUILDSTER_OS">
                <case check="Windows">MinGW Makefiles</case>
                <case check="Linux">Unix Makefiles</case>
                <case check="Darwin">Xcode</case>
                <default><quit></quit></default>
              </switch>
            </generator>
            <source>..</source>
          </cmake>
        </build>
        <exports>
          <export type="other">
            <key>SFML_CMAKE</key>
            <value><install></install>/share/SFML/cmake/Modules</value>
          </export>
          <export type="all">
            <key>SFML_ROOT</key>
            <value><install></install></value>
          </export>
          <export type="headers">
            <key>SFML_INCLUDE_DIR</key>
            <value><install></install>/include</value>
          </export>
          <export type="libraries">
            <key>SFML_LIBRARIES</key>
            <value><install></install>/lib</value>
          </export>
        </exports>
        <imports>
          <import>glew</import>
        </imports>
      </dependency>
      <dependency>
        <label>sfgui</label>
        <subpath>build</subpath>
        <remote>
          <url>https://github.com/TankOs/SFGUI.git</url>
          <git_repo>
            <branch>0.2.3</branch>
            <credentials>
              <username>username</username>
              <password>password</password>
            </credentials>
          </git_repo>
        </remote>
        <build>
          <arguments>
            <argument>-DCMAKE_POLICY_DEFAULT_CMP0074=NEW</argument>
            <argument>-DCMAKE_MODULE_PATH=<data id="SFML_CMAKE"></data></argument>
            <argument>-DSFGUI_BUILD_EXAMPLES=false</argument>
          </arguments>
          <cmake>
            <generator>
              <switch id="BUILDSTER_OS">
                <case check="Windows">MinGW Makefiles</case>
                <case check="Linux">Unix Makefiles</case>
                <case check="Darwin">Xcode</case>
                <default><quit></quit></default>
              </switch>
            </generator>
            <source>..</source>
          </cmake>
        </build>
        <exports>
          <export type="all">
            <key>SFGUI_ROOT</key>
            <value><install></install></value>
          </export>
          <export type="headers">
            <key>SFGUI_INCLUDE_DIR</key>
            <value><install></install>/include</value>
          </export>
          <export type="libraries">
            <key>SFGUI_LIBRARIES</key>
            <value><install></install>/lib</value>
          </export>
        </exports>
        <imports>
          <import>glew</import>
          <import>sfml</import>
        </imports>
      </dependency>
      <dependency>
        <label>assimp</label>
        <subpath>build</subpath>
        <remote>
          <url>https://github.com/assimp/assimp.git</url>
          <git_repo>
            <branch>v3.1.1</branch>
            <credentials>
              <username>username</username>
              <password>password</password>
            </credentials>
          </git_repo>
        </remote>
        <build>
          <arguments>
            <argument>-DCMAKE_POLICY_DEFAULT_CMP0074=NEW</argument>
          </arguments>
          <cmake>
            <generator>
              <switch id="BUILDSTER_OS">
                <case check="Windows">MinGW Makefiles</case>
                <case check="Linux">Unix Makefiles</case>
                <case check="Darwin">Xcode</case>
                <default><quit></quit></default>
              </switch>
            </generator>
            <source>..</source>
          </cmake>
        </build>
        <exports>
          <export type="all">
            <key>ASSIMP_ROOT</key>
            <value><install></install></value>
          </export>
          <export type="headers">
            <key>ASSIMP_INCLUDE_DIR</key>
            <value><install></install>/include</value>
          </export>
          <export type="libraries">
            <key>ASSIMP_LIBRARIES</key>
            <value><install></install>/lib</value>
          </export>
        </exports>
        <imports></imports>
      </dependency>
      <dependency>
        <label>glm</label>
        <subpath>build</subpath>
        <remote>
          <url>https://github.com/g-truc/glm.git</url>
          <git_repo>
            <branch>0.9.9.5</branch>
            <credentials>
              <username>username</username>
              <password>password</password>
            </credentials>
          </git_repo>
        </remote>
        <build>
          <arguments>
            <argument>-DCMAKE_POLICY_DEFAULT_CMP0074=NEW</argument>
            <argument>-DGLM_TEST_ENABLE=OFF</argument>
          </arguments>
          <cmake>
            <generator>
              <switch id="BUILDSTER_OS">
                <case check="Windows">MinGW Makefiles</case>
                <case check="Linux">Unix Makefiles</case>
                <case check="Darwin">Xcode</case>
                <default><quit></quit></default>
              </switch>
            </generator>
            <source>..</source>
          </cmake>
        </build>
        <exports>
          <export type="all">
            <key>GLM_ROOT</key>
            <value><install></install></value>
          </export>
          <export type="headers">
            <key>GLM_INCLUDE_DIR</key>
            <value><install></install>/include</value>
          </export>
          <export type="libraries">
            <key>GLM_LIBRARIES</key>
            <value><install></install>/lib</value>
          </export>
        </exports>
        <imports></imports>
      </dependency>
      <dependency>
        <label>devil</label>
        <subpath>DevIL/build</subpath>
        <remote>
          <url>https://github.com/DentonW/DevIL.git</url>
          <git_repo>
            <branch>v1.8.0</branch>
            <credentials>
              <username>username</username>
              <password>password</password>
            </credentials>
          </git_repo>
        </remote>
        <build>
          <arguments>
            <argument>-DCMAKE_POLICY_DEFAULT_CMP0074=NEW</argument>
          </arguments>
          <cmake>
            <generator>
              <switch id="BUILDSTER_OS">
                <case check="Windows">MinGW Makefiles</case>
                <case check="Linux">Unix Makefiles</case>
                <case check="Darwin">Xcode</case>
                <default><quit></quit></default>
              </switch>
            </generator>
            <source>..</source>
          </cmake>
        </build>
        <exports>
          <export type="all">
            <key>DEVIL_ROOT</key>
            <value><install></install></value>
          </export>
          <export type="headers">
            <key>DEVIL_INCLUDE_DIR</key>
            <value><install></install>/include</value>
          </export>
          <export type="libraries">
            <key>DEVIL_LIBRARIES</key>
            <value><install></install>/lib</value>
          </export>
        </exports>
        <imports></imports>
      </dependency>
    </dependencies>
    <targets>
      <target type="executable">
        <label>engine</label>
        <generator>
          <switch id="BUILDSTER_OS">
            <case check="Windows">CodeBlocks - MinGW Makefiles</case>
            <case check="Linux">CodeBlocks - Unix Makefiles</case>
            <case check="Darwin">Xcode</case>
            <default><quit></quit></default>
          </switch>
        </generator>
        <definitions></definitions>
         <packages>
           <package>
            <label>OpenGL</label>
            <exports>
              <export type="headers">
                <key>OPENGL_INCLUDE_DIR</key>
                <value></value>
              </export>
              <export type="libraries">
                <key>OPENGL_LIBRARIES</key>
                <value></value>
              </export>
            </exports>
          </package>
        </packages>
        <links>
          <link>*IL*</link>
          <link>*assimp*</link>
          <link>*sfgui*</link>
          <link>*sfml-graphics*</link>
          <link>*sfml-window*</link>
          <link>*sfml-audio*</link>
          <link>*sfml-network*</link>
          <link>*sfml-system*</link>
          <link>*glew*</link>
        </links>
        <imports>
          <import>devil</import>
          <import>glm</import>
          <import>assimp</import>
          <import>sfgui</import>
          <import>sfml</import>
          <import>glew</import>
        </imports>
      </target>
    </targets>
    <post timing="build">
      <build>
        <shells>
          <shell>
            <work><distribution></distribution></work>
            <commands>
              <command>
                <copy>
                  <from><distribution></distribution>/../../shaders</from>
                  <to><distribution></distribution></to>
                </copy>
              </command>
              <command>
                <copy>
                  <from><distribution></distribution>/../../models-textures</from>
                  <to><distribution></distribution></to>
                </copy>
              </command>
            </commands>
          </shell>
        </shells>
      </build>
    </post>
  </project>
</buildster>
